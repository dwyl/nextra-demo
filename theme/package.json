{
  "name": "nextra-theme-docs",
  "version": "3.0.8",
  "description": "A Nextra theme for documentation sites.",
  "repository": "https://github.com/shuding/nextra",
  "author": "Shu Ding <g@shud.in>",
  "license": "MIT",
  "exports": {
    "./style.css": "./style.css",
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.mts"
    }
  },
  "types": "./dist/index.d.mts",
  "files": [
    "dist",
    "style.css"
  ],
  "workspaces": [
    "theme"
  ],
  "scripts": {
    "build": "tsup",
    "build:all": "pnpm build && pnpm build:tailwind",
    "build:tailwind": "pnpm postcss css/styles.css -o style.css --verbose",
    "clean": "rimraf ./dist ./style.css",
    "dev": "concurrently \"pnpm dev:layout\" \"pnpm dev:tailwind\"",
    "dev:layout": "tsup --watch",
    "dev:tailwind": "TAILWIND_MODE=watch pnpm postcss css/styles.css -o style.css --watch",
    "prepublishOnly": "pnpm build:all",
    "test": "echo ❗ No tests, previous tests were moved to the `nextra` package",
    "types": "tsup --dts-only",
    "types:check": "tsc --noEmit"
  },
  "peerDependencies": {
    "next": ">=13",
    "nextra": "workspace:*",
    "react": ">=18",
    "react-dom": ">=18"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "focus-visible": "^5.2.0",
    "git-url-parse": "^13.1.0",
    "intersection-observer": "^0.12.2",
    "match-sorter": "^6.3.1",
    "next-seo": "^6.0.0",
    "@headlessui/react": "^2.1.2",
    "clsx": "^2.0.0",
    "escape-string-regexp": "^5.0.0",
    "flexsearch": "^0.7.43",
    "next-themes": "^0.3.0",
    "scroll-into-view-if-needed": "^3.1.0",
    "zod": "^3.22.3",
    "better-react-mathjax": "^2.0.3"
  },
  "devDependencies": {
    "@types/flexsearch": "^0.7.3",
    "@types/git-url-parse": "^9.0.1",
    "tsup": "^8.1.0",
    "@tailwindcss/nesting": "^0.0.0-insiders.565cd3e",
    "@testing-library/react": "^16.0.0",
    "@types/react": "^18.2.23",
    "@types/react-dom": "^18.2.8",
    "@vitejs/plugin-react": "^4.1.0",
    "concurrently": "^9.0.0",
    "jsdom": "^25.0.0",
    "next": "^14.2.5",
    "nextra": "workspace:*",
    "postcss": "^8.4.33",
    "postcss-cli": "^11.0.0",
    "postcss-import": "^16.0.0",
    "postcss-lightningcss": "^1.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.4.1",
    "vitest": "^2.0.3",
    "@svgr/webpack": "^8.0.1"
  },
  "sideEffects": [
    "./src/polyfill.ts"
  ]
}
